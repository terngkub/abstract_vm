; test01_overflow_underflow

; int8
push int8(127)			;ok
push int8(128)			;overflow
push int8(-128)			;ok
push int8(-129)			;underflow

; int16
push int16(32767)		;ok
push int16(32768)		;overflow
push int16(-32768)		;ok
push int16(-32769)		;underflow

; int32
push int32(2147483647)	;ok
push int32(2147483648)	;overflow
push int32(-2147483648) ;ok
push int32(-2147483649)	;underflow

; float
push float(340282000000000000000000000000000000000)		;ok
push float(340283000000000000000000000000000000000)		;overflow
push float(-340282000000000000000000000000000000000)	;ok
push float(-340283000000000000000000000000000000000)	;overflow

; double
push double()	;ok
push double()	;overflow
push double(-)	;ok
push double(-)	;underflow

exit