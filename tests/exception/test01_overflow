; test01_overflow_underflow

; int8
push int8(127)			;ok
push int8(128)			;positive overflow
push int8(-128)			;ok
push int8(-129)			;negative overflow

; int16
push int16(32767)		;ok
push int16(32768)		;positive overflow
push int16(-32768)		;ok
push int16(-32769)		;negative overflow

; int32
push int32(2147483647)	;ok
push int32(2147483648)	;positive overflow
push int32(-2147483648) ;ok
push int32(-2147483649)	;negative overflow

; float
push float(340282000000000000000000000000000000000)		;ok
push float(340283000000000000000000000000000000000)		;positive overflow
push float(-340282000000000000000000000000000000000)	;ok
push float(-340283000000000000000000000000000000000)	;negative overflow

; double
push double()	;ok
push double()	;positive overflow
push double(-)	;ok
push double(-)	;negative overflow

exit